    "tokenColors": [
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},

        {
			"scope": "punctuation.definition.comment",
			"settings": {
				"foreground": "#5B6268"
			}
		},
        {
			"scope": "comment",
			"settings": {
				"foreground": "#5B6268"
			}
		},

        {
			"scope": "punctuation.definition.directive",
			"settings": {
				"foreground": "#4BC4F7"
			}
		},
        {
			"scope": "keyword.control.directive",
			"settings": {
				"foreground": "#4BC4F7"
			}
		},
        {
			"scope": "meta.preprocessor",
			"settings": {
				"foreground": "#4BC4F7"
			}
		},

        {
			"scope": "punctuation.definition.string",
			"settings": {
				"foreground": "#98BE65"
			}
		},
        {
			"scope": "string",
			"settings": {
				"foreground": "#98BE65"
			}
		},	

        {
